{
    "__notes__": [
        "This config point to precompiled binaries, most of which for 64bit platforms. Consider overriding this in your config.",
        "mkbootimg and unpackimg are necessary if you have boot.make = true, or recovery.make = true",
        "heimdall is necessary if you are going to use 'incept plant' command for samsung devices",
        "make_ext4fs is necessary if you have cache.make = true",
        "adb is necessary if you use 'incept learn' command"
     ],
    "common": {
        "tools": {
            "make_ext4fs": {
                "bin": "/usr/bin/make_ext4fs"
            },
            "adb": {
                "bin": "/usr/bin/adb"
            },
            "update-binary": {
                "bin": "bin/update-binary"
            },
            "mkbootimg": {
                "bin": "bin/mkbootimg"
            },
            "unpackbootimg": {
                "bin": "bin/unpackbootimg"
            },
            "apktool": {
                "bin": "bin/apktool_2.0.0b7.jar",
                "frameworks_dir": null
            },
            "signapk": {
                "bin": "bin/signapk.jar",
                "keys": {
                    "test": {
                        "private": "keys/testkey.pk8",
                        "public": "keys/testkey.x509.pem"
                    }
                }
            },
            "java": {
                "bin": "/usr/bin/java"
            },
            "heimdall": {
                "bin": "/usr/bin/heimdall"
            }
        }
    },
    "update": {
        "keys": "test"
    }
}